[user]
	name = Vaios Karampinis
	email = oden@oden.gr
[github]
	user = vkarampinis
[core]
	excludesfile = /Users/Oden/.gitignore_global
	preloadindex = true
	fscache = true
[color]
	ui = auto
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[color "branch"]
	local = yellow
	remote = green
	current = cyan
[difftool "sourcetree"]
	cmd = /Applications/DiffFork.app/Contents/SharedSupport/Support/bin/difffork -w \"$LOCAL\" \"$REMOTE\"
	path =
[mergetool "sourcetree"]
	cmd = /Applications/p4merge.app/Contents/MacOS/p4merge \"$BASE\" \"$LOCAL\" \"$REMOTE\" \"$MERGED\"
	trustExitCode = true
[gc]
	auto = 256
[push]
	default = simple
